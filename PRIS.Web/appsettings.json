{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=PrisDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
  "WriteTo": [
    { "Name": "Console" },
    {
      "Name": "MSSqlServer",
      "Args": {
        "connectionString": "Server=(localdb)\\mssqllocaldb;Database=PrisDb;Trusted_Connection=True;MultipleActiveResultSets=true",
        "sinkOptions": {
          "tableName": "Logs",
          "autoCreateSqlTable": true
        }
      }
    },
    {
      "Name": "Seq",
      "Args": {
        "serverUrl": "http://localhost:8081/"
      }
    }
  ]

}
